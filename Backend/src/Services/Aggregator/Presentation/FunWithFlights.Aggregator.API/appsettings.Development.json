{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateLimitOptions": {
    "PermitLimit": 10000,
    "Window": 10,
    "QueueLimit": 2
  },
