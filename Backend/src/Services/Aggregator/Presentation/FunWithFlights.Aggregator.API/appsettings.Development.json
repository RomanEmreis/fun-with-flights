    "RetryCount": 5,
    "MedianFirstRetryDelay": 1
  },
  "Aspire": {
    "StackExchange": {
      "Redis": {
        "ConfigurationOptions": {
          "ConnectTimeout": 3000,
          "ConnectRetry": 2
        },
        "HealthChecks": true,
        "Tracing": true
      }
    }
  }
